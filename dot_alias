# -*- coding:utf-8; mode:shell-script  -*-
alias pull='git pull'
alias push='git push'
alias st='git status'
alias stt='git status -uno'
alias git-submodule-rm="git rm-submodule --no-commit"
alias gitu="git unstage"
alias gl="git clone"
# リモートからフェッチ(タグを更新し、削除済みブランチを削除)してpullする
alias gu="git fetch --tags --prune && git pull origin"
# this branch
alias tb="git symbolic-ref --short HEAD | tr -d \"\\n\""
# git status(st) lfs
alias stl='git lfs status'
alias push-u-this-branch='git push -u origin HEAD'
alias 1="awk '{print \$1}'"
alias 2="awk '{print \$2}'"
alias 3="awk '{print \$3}'"
alias 4="awk '{print \$4}'"
alias 5="awk '{print \$5}'"
alias 6="awk '{print \$6}'"
alias 7="awk '{print \$7}'"
alias 8="awk '{print \$8}'"
alias 9="awk '{print \$9}'"
alias x=xargs
alias today='date "+%Y%m%d"'
alias sam=/usr/local/bin/sam
alias z=zellij
alias d=docker
alias doco=docker-compose
alias cdl='cd $(command ls -tr|tail -n 1)'
alias cdg='cd $(git rev-parse --show-toplevel)'
alias e='emacs -q -nw'
alias gitco='git checkout --'
alias jl=jless
alias tf=terraform
alias git-add-safe-directory='git config --global --add safe.directory $(pwd)'
